pipeline {
   agent any

   stages {
      stage('Hello') {
         steps {
            echo 'Hello World'
         }
     }
     stage('getcommand') {
         steps {
             powershell label: '', script: 'get-command'
             
         }
         
         
     }
     stage('ipconfig') {
         steps {
             powershell label: '', script: 'hostname'
             
         }
   }
   
}
